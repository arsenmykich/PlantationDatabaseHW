@model IEnumerable<string>

<h1>Consumers with Distinct Products</h1>

<table class="table">
    <thead>
        <tr>
            <th>Customer ID</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var customerId in Model)
        {
            <tr>
                <td>@customerId</td>
            </tr>
        }
    </tbody>
</table>
