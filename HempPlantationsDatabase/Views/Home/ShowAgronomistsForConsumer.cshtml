@model IEnumerable<int>

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Agronomists for Consumer Tastings</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            margin: 20px;
        }

        h1 {
            color: #333;
        }

        table {
            width: 100%;
            border-collapse: collapse;
            margin-top: 20px;
        }

        th, td {
            border: 1px solid #ddd;
            padding: 8px;
            text-align: left;
        }

        th {
            background-color: #f2f2f2;
        }

        a {
            color: #007bff;
            text-decoration: none;
        }

            a:hover {
                text-decoration: underline;
            }
    </style>
</head>
<body>
    <h1>Agronomists for Consumer Tastings</h1>

    <table>
        <thead>
            <tr>
                <th>Agronomist ID</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var agronomistId in Model)
            {
                <tr>
                    <td>@agronomistId</td>
                </tr>
            }
        </tbody>
    </table>

    <p>
        <a href="/">Back to Main Menu</a>
    </p>
</body>
<a asp-action="Index">Back to Main Menu</a>
</html>
